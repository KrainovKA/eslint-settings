{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["prettier", "react"],
	"rules": {
		"eqeqeq": "error",
		"curly": "error",
		"no-var": "error",
		"no-console": "error",
		"dot-notation": "error",
		"one-var": ["error", "never"]
	}
}
